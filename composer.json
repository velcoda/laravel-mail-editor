{
    "name": "velcoda/laravel-mail-editor",
    "description": "Laravel Mailable Editor",
    "license": "MIT",
    "authors": [
        {
            "name": "yassine qoraiche",
            "email": "qoraicheofficiel@hotmail.com",
            "homepage": "https://github.com/qoraiche"
        }
    ],
    "homepage": "https://github.com/Qoraiche/laravel-mail-editor",
    "keywords": [
        "Laravel",
        "mailEclipse",
        "mailable-editor",
        "laravel-mail",
        "laravel-mailable",
        "mailable",
        "markdown"
    ],
    "require": {
        "illuminate/support": "^v12.30.1",
        "velcoda/mocker": "dev-master",
        "laravel/legacy-factories": "^v1.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.39",
        "mockery/mockery": "^1.6.12",
        "orchestra/testbench": "^v10.6.0"
    },
    "autoload": {
        "psr-4": {
            "Qoraiche\\MailEclipse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qoraiche\\MailEclipse\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Qoraiche\\MailEclipse\\MailEclipseServiceProvider"
            ],
            "aliases": {
                "MailEclipse": "Qoraiche\\MailEclipse\\Facades\\MailEclipse"
            }
        }
    }
}
